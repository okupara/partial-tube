{
  "name": "@partial-tube/frontend",
  "version": "0.0.0",
  "description": "> TODO: description",
  "author": "okupara <okuharakensuke@gmail.com>",
  "homepage": "",
  "license": "ISC",
  "main": "dist/bundled.js",
  "directories": {
    "test": "__tests__"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "start": "npm run dev",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:watchAll": "jest --watchAll",
    "cypress": "cypress open",
    "storybook": "start-storybook -p 6006 -s ./hosting",
    "dev": "npm run format && npm run gen-html:dev && webpack-dev-server --open --config webpack.config.dev.js",
    "check": "npm run format && npm run tsc",
    "format": "prettier --write './**/*.{js,ts,tsx,json,html}'",
    "gen-html:dev": "NODE_ENV=development generate-html -s src/index.html -o hosting/index.html",
    "tsc": "tsc --noEmit"
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.5.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@cypress/webpack-preprocessor": "^4.1.0",
    "@partial-tube/helper-cli": "^0.0.0",
    "@storybook/addon-actions": "^5.2.4",
    "@storybook/addon-info": "^5.2.4",
    "@storybook/addon-links": "^5.2.4",
    "@storybook/addons": "^5.2.4",
    "@storybook/cli": "^5.2.4",
    "@storybook/react": "^5.2.4",
    "@testing-library/jest-dom": "^5.0.0",
    "@testing-library/react": "^9.4.0",
    "@types/firebase": "^3.2.1",
    "@types/graphql": "^14.2.3",
    "@types/jest": "^24.0.15",
    "@types/lodash-es": "^4.17.3",
    "@types/node": "^12.6.2",
    "@types/react": "^16.9.18",
    "@types/react-dom": "^16.9.5",
    "@types/react-router-dom": "^4.3.5",
    "@types/storybook__react": "^4.0.2",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-dynamic-import-node": "^2.3.0",
    "core-js": "^3.6.4",
    "cypress": "^3.4.1",
    "ejs": "^2.6.2",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "typescript": "^3.7.5",
    "wait-for-expect": "^2.0.0",
    "webpack": "^4.35.3",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.2",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.0-beta.0",
    "@apollo/react-testing": "^3.1.0-beta.0",
    "@babel/polyfill": "^7.4.4",
    "@babel/runtime": "^7.6.3",
    "@material-ui/core": "^4.2.0",
    "@material-ui/icons": "^4.2.1",
    "@material-ui/styles": "^4.2.0",
    "@material-ui/types": "^4.1.1",
    "@partial-tube/domain": "^0.0.0",
    "@testing-library/react-hooks": "^2.0.1",
    "@types/youtube": "^0.0.38",
    "apollo-cache-inmemory": "^1.6.2",
    "apollo-client": "^2.6.4",
    "apollo-link": "^1.2.12",
    "apollo-link-error": "^1.1.11",
    "apollo-link-http": "^1.5.15",
    "apollo-link-rest": "^0.7.3",
    "firebase": "^6.6.0",
    "formik": "^1.5.8",
    "formik-material-ui": "^0.0.20",
    "fp-ts": "^2.0.3",
    "graphql": "^14.4.2",
    "graphql-anywhere": "^4.2.4",
    "graphql-tag": "^2.10.1",
    "io-ts": "^2.0.0",
    "lodash-es": "^4.17.15",
    "react-hot-loader": "^4.12.6",
    "react-router-dom": "^5.0.1",
    "react-test-renderer": "^16.9.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "tsx",
      "ts",
      "js"
    ],
    "moduleNameMapper": {
      "^components/(.+)": "<rootDir>/src/components/$1",
      "^hooks/(.+)": "<rootDir>/src/hooks/$1",
      "^utils/(.+)": "<rootDir>/src/utils/$1",
      "^containers/(.*)": "<rootDir>/src/containers/$1",
      "^__mocks__/(.+)": "<rootDir>/src/__mocks__/$1"
    },
    "transformIgnorePatterns": [
      "/node_modules/(?!lodash-es).+\\.js$"
    ],
    "transform": {
      "^.+\\.[t|j]sx?$": "babel-jest"
    },
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom/extend-expect"
    ],
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/src/**/__tests__/**/*.test.ts?(x)"
    ]
  }
}
